ACTOR BowArrow : Weapon 11149
{
    Inventory.PickupMessage "You picked up the bow."
    Obituary "%o was perforated by %k's bow."
    AttackSound "weapons/bowfire"
    Weapon.AmmoType1 "RocketAmmo"
    Weapon.AmmoGive1 20
    Weapon.AmmoUse1 1
	+NOAUTOAIM
	+DONTBOB
    Radius 30
	Height 10
    Scale .20
    States
    {
		Ready:
		   BOWA H 0
		   BOWA H 1 A_WeaponReady
		   Goto Ready+1
		Deselect:
		   BOWA H 1 A_Lower
		Loop
		Select:
		   BOWA H 0	A_Playsound("weapons/bowup")
		   BOWA H 1 A_Raise
		   Goto Select+1
		Fire:
		   BOWA HIJKL 4
		   BOWA H 0
		   BOWA L 4 A_Playsound("weapons/bowdraw")
		Goto Hold
		Hold:
		   BOWA M 1
		   BOWA M 0 A_Refire
		   BOWA O 0 A_FireCustomMissile("ArrowShot", 0, 0, 8, 8)
		   BOWA O 0 A_GunFlash
		   BOWA P 15
		   BOWA G 4
		   Goto Ready
			   Flash:
		   TNT1 A 19
		   Stop
    }
}

