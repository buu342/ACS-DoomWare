{
	Health 3000
	Radius 30
	Height 62
	Mass 1000
	Speed 24
	PainChance 40
	Monster
	MinMissileChance 1
	+BOSS
	+MISSILEMORE
	+MISSILEEVENMORE
	+FLOORCLIP
	+NORADIUSDMG
	+DONTMORPH
	+BOSSDEATH
	+NOINFIGHTING
