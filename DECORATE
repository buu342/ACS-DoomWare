ACTOR BusPlayer : DoomPlayer
{
	player.forwardmove 0
	player.sidemove 0.5
	player.jumpz 0
	Health 100
	Radius 16
	Height 30
	Mass 100
	Player.ColorRange 112, 127
	PainSound ""
	DeathSound ""
	Player.MorphWeapon "BusWeapon"
	Player.SoundClass "Kart"
	+NOBLOOD
	+pushable
	+NOSKIN
	States
	{
		Missile:
			Goto See
		See:
			Spawn:
			BUSP A 5
			Loop
		Death:
			MISL B 5 bright A_Playsound("weapons/rocklx")
			MISL C 5 bright
			MISL D 5 bright
			TNT2 A 5 bright
			TNT2 A -1
			Stop
		XDeath:
			MISL B 5 bright A_Playsound("weapons/rocklx")
			MISL C 5 bright
			MISL D 5 bright
			TNT2 A 5 bright
			TNT2 A -1
			Stop
	}
}

