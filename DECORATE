	{
		Ready:
			SUIC A 1 A_WeaponReady
			Loop
		Deselect:
			SUIC A 1 A_Lower
			Loop
		Select:
			SUIC A 1 A_Raise
			Loop
		Fire:
			SUIC A 4
			SUIC B 6
			SUIC B 0
			SUIC C 4 A_PlaySound("weapons/pistol", CHAN_WEAPON)
			SUIC C 0 A_DIE
			SUIC B 5 A_ReFire
			Goto Ready
		Flash:
			SUIC A 7 Bright A_Light1
			Goto LightDone
			SUIC A 7 Bright A_Light1
			Goto LightDone
		Spawn:
			PIST A -1
			Stop
	}
}

