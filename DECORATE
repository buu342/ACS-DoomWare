ACTOR OutriderWeapon : DoomWeapon
{
	Weapon.SelectionOrder 2000
	+WEAPON.DONTBOB
	+WEAPON.NOAUTOFIRE
	States
	{
		Ready:
			ORVW A 0 A_Recoil(-1.5)
			ORVW A 1 A_WeaponReady
			ORVW A 0 A_PlaySound("DoomWare/SnowSpeederEngine", 6, 0.5)
			loop
		Deselect:
			ORVW A 0 A_StopSound(6)
			ORVW A 1 A_Lower
			loop
		Select:
			ORVW A 1 A_Raise
			Loop
		Fire:
			ORVW A 1
			goto Ready
		AltFire:
			goto Ready
	}
}

