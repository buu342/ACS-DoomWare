ACTOR TinOre 11142
{
	Health 40
	Radius 16
	Height 72
	Mass 1000000
	Speed 0
	PainChance 256
	SeeSound ""
	PainSound "DoomWare/PickaxeHit"
	DeathSound "DoomWare/PickaxeCollect"
	ActiveSound ""
	+FLOORCLIP
	+NoBlood
	+SHOOTABLE
	+SOLID
	States
	{
		Spawn:
			RSOR A 10 A_Look
			Loop
		See:
			RSOR A 3
			Loop
		Pain:
			RSOR A 0 A_PAIN
			goto See
		Death:
			RSOR B 0 A_GiveToTarget("TinOreItem",1)
			RSOR B 0 A_Scream
			RSOR B 0 A_ChangeFlag("shootable",1)
			RSOR B 0 A_ChangeFlag("SOLID",1)
			RSOR B 0 A_CustomMissile("SolidCorpse")
			RSOR B 175 A_Scream
			stop
	}
}

