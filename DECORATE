ACTOR GrenadeLauncher : DoomWeapon 5011
{
	Game Doom
	SpawnID 163
	Radius 20
	Height 16
	Weapon.Selectionorder 2500
	+WEAPON.NOAUTOFIRE
	+WEAPON.NOAUTOAIM
	Weapon.AmmoUse 1
	Weapon.AmmoGive 2
	Weapon.AmmoType "RocketAmmo"
	Weapon.Kickback 100
	Inventory.PickupMessage "$PICKUP_GRENADELAUNCHER"
	States
	{
		Spawn:
			GLAU A -1
			stop
		Ready:
			GRLG A 1 A_WeaponReady
			loop
		Deselect:
			GRLG A 1 A_Lower
			loop
		Select:
			GRLG A 1 A_Raise
			loop
		Fire:
			GRLG B 8 A_GunFlash
			GRLG B 12 A_FireCustomMissile ( "DWGrenade", 0, 1, 0, 0, 0, 6.328125 )
			GRLG B 0 A_ReFire
			goto Ready
		Flash:
			GRLF A	3 bright A_Light1
			GRLF B	4 bright
			GRLF C 4 bright A_Light2
			GRLF D 4 bright A_Light2
			Goto LightDone
	}
}

